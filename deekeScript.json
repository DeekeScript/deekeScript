{
  "name": "Deeke私信",
  "icon": "images/test/app.png",
  "head": "images/test/app.png",
  "settingTopBg": "images/test/setting-top.png",
  "versionCode": "101",
  "versionName": "1.0.1",
  "host": "https://home.deeke.top",
  "switchSetting": {
    "alipay": true
  },
  "groups": [
    {
      "title": "某音系列",
      "titleHidden": false,
      "hidden": false,
      "methods": [
        {
          "title": "精准私信",
          "icon": "images/dy_uid.png",
          "columns": 24,
          "jsFile": "script/task/dy_uid.js",
          "settingPage": {
            "params": [
              {
                "type": "textArea",
                "label": "地址信息",
                "name": "toker_dy_address",
                "lines": 5,
                "value": "",
                "notice": "* 提供随机搜索关键词，每行一个"
              },
              {
                "type": "textArea",
                "label": "行业信息",
                "name": "toker_dy_shop",
                "lines": 5,
                "value": "",
                "notice": "* 提供随机搜索关键词，每行一个"
              },
              {
                "type": "numberRange",
                "label": "操作平均间隔",
                "name": "toker_dy_second",
                "max": "120",
                "min": 0,
                "value": "10"
              },
              {
                "type": "number",
                "label": "私信次数",
                "name": "toker_dy_private_count",
                "value": 10
              },
              {
                "type": "radio",
                "label": "粉丝数量",
                "name": "toker_dy_fans_count",
                "columnCount": 3,
                "options": [
                  {
                    "label": "1000以内",
                    "value": 0
                  },
                  {
                    "label": "1000-1万",
                    "value": 1
                  },
                  {
                    "label": "1万-10万",
                    "value": 2
                  },
                  {
                    "label": "10万-100万",
                    "value": 3
                  },
                  {
                    "label": "100万以上",
                    "value": 4
                  }
                ],
                "value": "0"
              },
              {
                "type": "radio",
                "label": "用户类型",
                "name": "toker_dy_type",
                "options": [
                  {
                    "label": "不限制",
                    "value": 0
                  },
                  {
                    "label": "普通用户",
                    "value": 1
                  },
                  {
                    "label": "企业认证",
                    "value": 2
                  },
                  {
                    "label": "个人认证",
                    "value": 3
                  }
                ],
                "value": "0"
              },
              {
                "type": "number",
                "label": "运行时间（分钟）",
                "name": "toker_dy_run_minute",
                "value": 10
              },
              {
                "type": "numberRange",
                "label": "每轮休息时间（分钟）",
                "name": "toker_dy_sleep_minute",
                "max": 120,
                "min": 0,
                "step": 1,
                "value": 5
              },
              {
                "type": "notice",
                "title": "* 关键词将根据“地址信息”和“行业信息”随机生成，每轮休眠后自动切换新的关键词"
              }
            ]
          }
        }
      ]
    }
  ],
  "bottomMenus": [
    {
      "title": "{NAME}",
      "icon": "images/test/home.png",
      "banner": "images/test/home-top.png",
      "type": "home"
    },
    {
      "title": "话术设置",
      "icon": "images/test/speech.png",
      "type": "speech"
    },
    {
      "title": "系统设置",
      "icon": "images/test/setting.png",
      "type": "setting"
    }
  ],
  "settingLists": [
    {
      "title": "联系客服",
      "icon": "images/test/kefu.png",
      "type": "customerService",
      "description": "客服微信：无",
      "hidden": false
    },
    {
      "title": "上传日志",
      "icon": "images/test/upload.png",
      "type": "uploadLog",
      "url": "/dke/uploadLog"
    },
    {
      "title": "系统升级",
      "icon": "images/test/update.png",
      "type": "updateApp",
      "url": "/dke/updateApp"
    },
    {
      "title": "清理数据",
      "icon": "images/test/clear.png",
      "description": "确定清理嘛？",
      "type": "clear"
    },
    {
      "title": "举报中心",
      "icon": "images/test/jubao.png",
      "type": "qiwei",
      "url": "https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=910d7c7c-d74a-4ce5-a070-0f3383b1f6ee",
      "hidden": false,
      "settingPage": {
        "params": [
          {
            "type": "notice",
            "title": "如果我们的代理商有侵犯到你的权益，请立即举报！",
            "hidden": false
          },
          {
            "type": "text",
            "label": "举报人",
            "value": "",
            "name": "qiwei_name"
          },
          {
            "type": "text",
            "label": "举报人手机号",
            "value": "",
            "name": "qiwei_mobile"
          },
          {
            "type": "text",
            "label": "举报人身份证号码",
            "value": "",
            "name": "qiwei_card_no"
          },
          {
            "type": "text",
            "label": "举报内容",
            "value": "",
            "hint": "请详细描述你举报该软件的原因，请不要恶意举报！",
            "name": "qiwei_content",
            "lines": 3
          }
        ]
      }
    }
  ],
  "apis": [
    {
      "type": "bind",
      "url": "/dke/login"
    },
    {
      "type": "checkBind",
      "url": "/dke/checkBind"
    },
    {
      "type": "aiSpeechToken",
      "url": "/dke/getBaiduToken"
    },
    {
      "type": "config",
      "url": "/dkee/config"
    },
    {
      "type": "getToken",
      "url": "/alipay/getToken"
    },
    {
      "type": "createOrder",
      "url": "/alipay/createOrder"
    }
  ]
}